{
  "name": "@tillhub/javascript-sdk",
  "version": "1.16.0",
  "description": "The JavaScript (Browser) SDK for the Tillhub API.",
  "keywords": [],
  "main": "dist/lib/tillhub-js.js",
  "module": "dist/lib/tillhub-js.js",
  "typings": "dist/types/tillhub-js.d.ts",
  "files": [
    "package.json",
    "package-lock.json",
    "dist",
    "CHANGELOG.md"
  ],
  "author": "Robert Jefe Lindstaedt <robert.lindstaedt@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tillhub/tillhub-sdk-javascript.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build:main": "tsc -p tsconfig.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:bundle": "tsc --module commonjs",
    "build": "npm run clean && npm run build:bundle && npm run build:docs",
    "clean": "rimraf dist docs",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "coverage": "codecov",
    "precommit": "git add -A && lint-staged",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once",
    "storybook": "start-storybook -p 9001 -c examples/.storybook"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 30
      }
    },
    "collectCoverage": true
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/github": "^5.0.0",
    "@semantic-release/npm": "^4.0.1",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/react": "^4.0.0-alpha.21",
    "@types/jest": "^23.1.6",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "browserify": "^16.2.2",
    "codecov": "^3.0.4",
    "colors": "^1.3.0",
    "commitizen": "^2.10.1",
    "common-tags": "^1.8.0",
    "core-js": "^2.5.7",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^6.0.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-localstorage-mock": "^2.2.0",
    "lint-staged": "^7.2.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.13.7",
    "prismjs": "^1.15.0",
    "prompt": "^1.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-prism": "^4.3.2",
    "replace-in-file": "^3.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.16.0",
    "rollup-plugin-visualizer": "^0.9.0",
    "semantic-release": "^15.9.12",
    "serialize-error": "^2.1.0",
    "storybook": "^1.0.0",
    "travis-deploy-once": "^5.0.1",
    "ts-jest": "^23.0.0",
    "ts-loader": "^5.0.0",
    "ts-node": "^7.0.0",
    "tsify": "^4.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^2.9.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "dist",
          "CHANGELOG.md"
        ],
        "message": "chore(release): Release ${nextRelease.version} - <%=new Date().toLocaleDateString('en-US', {year: 'numeric', month: 'short', day: 'numeric', hour: 'numeric', minute: 'numeric' })%> [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
